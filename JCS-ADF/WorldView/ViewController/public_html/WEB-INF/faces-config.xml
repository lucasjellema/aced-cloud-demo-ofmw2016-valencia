<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
  </application>
  <navigation-rule>
    <from-view-id>/main.jsf</from-view-id>
    <navigation-case>
      <from-outcome>map</from-outcome>
      <to-view-id>/WorldMap.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>news</from-outcome>
      <to-view-id>/WorldNews.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>catalog</from-outcome>
      <to-view-id>/CountriesCatalog.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>errors</from-outcome>
      <to-view-id>/WorldOfErrors.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/WorldNews.jsf</from-view-id>
    <navigation-case>
      <from-outcome>main</from-outcome>
      <to-view-id>/main.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/WorldMap.jsf</from-view-id>
    <navigation-case>
      <from-outcome>main</from-outcome>
      <to-view-id>/main.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/CountriesCatalog.jsf</from-view-id>
    <navigation-case>
      <from-outcome>main</from-outcome>
      <to-view-id>/main.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/WorldOfErrors.jsf</from-view-id>
    <navigation-case>
      <from-outcome>main</from-outcome>
      <to-view-id>/main.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>